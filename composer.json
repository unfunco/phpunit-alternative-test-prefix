{
    "name": "unfunco/phpunit-alternative-test-prefix",
    "description": "Listener for PHPUnit that allows for alternative test method prefixes to be specified within the PHPUnit configuration file.",
    "version": "0.1.1",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Morris",
            "email": "daniel@honestempire.com",
            "role": "Programmer"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.2"
    },
    "autoload": {
        "psr-4": {
            "Unfunco\\PHPUnit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Unfunco\\PHPUnit\\Test\\": "test/"
        }
    }
}
